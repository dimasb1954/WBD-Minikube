{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node src/index.ts",
    "start:nodemon": "npx nodemon -L -e",
    "dev": "npx nodemon -L src -e ts,json --exec \"npx tsx src/index.ts\"",
    "compile": "npx tsx src/index.ts",
    "build": "tsx",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:save": "prisma migrate save --name init --experimental",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "seed": "tsx prisma/seed.ts",
    "prunedb": "tsx prisma/prunedb.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "@vercel/blob": "^0.27.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.1",
    "express": "^4.21.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.13",
    "socket.io": "^4.8.1",
    "verbose": "^0.2.3",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.16",
    "@types/web-push": "^3.6.4",
    "nodemon": "^3.0.3",
    "prisma": "^5.9.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  },
  "author": "WBD",
  "license": "",
  "description": ""
}
